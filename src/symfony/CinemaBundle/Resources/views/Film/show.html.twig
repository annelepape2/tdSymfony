{% extends 'symfonyCinemaBundle::layout.html.twig' %}
{% block title %}{{ film.titre }}{% endblock %}
{% block body %}
<h1>{{ film.titre }}</h1>

<span>Réalisé par {{ film.auteur }}</span>

<p>{{ film.synopsis }}</p>




<p><a href="{{ path('page_films') }}">Retour</a></p>
{% endblock %}